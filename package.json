{
  "name": "@fastgpt/official-plugins",
  "version": "1.0.0",
  "description": "Official plugins for FastGPT",
  "scripts": {
    "build": "turbo run build",
    "pack": "turbo run pack",
    "type-check": "turbo run type-check --parallel",
    "biome-check": "turbo run biome-check --parallel",
    "postinstall": "lefthook install",
    "clean:node_modules": "tsx scripts/clean-node-modules.ts",
    "clean:dist": "tsx scripts/clean-dist.ts"
  },
  "keywords": [
    "fastgpt",
    "plugins",
    "official"
  ],
  "author": "FastGPT Team",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@fastgpt-plugin/cli": "catalog:",
    "@types/node": "catalog:",
    "lefthook": "catalog:",
    "tsx": "catalog:",
    "turbo": "catalog:",
    "vitest": "catalog:"
  }
}
