{
  "name": "duckduckgo",
  "version": "0.0.1",
  "description": "This is a FastGPT plugin",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx @fastgpt-plugin/cli build --minify",
    "build:dev": "npx @fastgpt-plugin/cli build",
    "pack": "npx @fastgpt-plugin/cli pack",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "biome-check": "biome check . --write"
  },
  "dependencies": {
    "duck-duck-scrape": "^2.2.7"
  },
  "devDependencies": {
    "@fastgpt-plugin/cli": "catalog:",
    "@fastgpt-plugin/helpers": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  }
}
